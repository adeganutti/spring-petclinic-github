pipeline {
    agent any

    stages {
        stage('paramethers') {
            steps {
                sh "echo $tipoBranch" 
                sh "echo $nomeBranch" 
            }
        }
        stage('create-branch') { 
            steps {
                sh "git checkout -b $tipoBranch/$nomeBranch"
                sh "git push origin $tipoBranch/$nomeBranch"      
            }
        }
    }
}
