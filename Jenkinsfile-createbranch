pipeline {
    agent any

    stages {
        stage('paramethers') {
            steps {
                sh "echo $tipoBranch" 
                sh "echo $nomeBranch" 
            }
        }
        stage('create-branch') { 
            steps {
                sh "git checkout -b $tipoBranch/$nomeBranch"
                sh "git push https://adeganutti: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@github.com/adeganutti/spring-petclinic-github.git"      
            }
        }
    }
}
